# Supabase (shared with seoul-location-services-app)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_DATABASE_URL=postgresql://postgres.your-project:password@host:6543/postgres

# OpenAI (for GPT-4o Vision extraction and explanations)
OPENAI_API_KEY=your_openai_api_key

# Upstash Redis (optional caching)
UPSTASH_URL=https://your-redis.upstash.io
UPSTASH_TOKEN=your_upstash_token

# Application
ENVIRONMENT=development
LOG_LEVEL=INFO
API_VERSION=v1

# Question extraction source
QUESTION_IMAGE_DIR=../data/raw/images
